{
  "parser": "@typescript-eslint/parser",
  "presets": [
    "@babel/env",
    "@babel/typescript"
  ],
  "extends": [
    "prettier"
  ],
  "plugins": [
    "react",
    "standard",
    "@typescript-eslint",
    "react-hooks"
  ],
  "rules": {
    "prettier/prettier": "error",
    "semi": "off",
    "no-unused-vars": [
      "off",
      {
        "vars": "local",
        "args": "none",
        "ignoreRestSiblings": false,
        "varsIgnorePattern": "React",
        "argsIgnorePattern": "^_",
        "caughtErrors": "none"
      }
    ]
  }
}
